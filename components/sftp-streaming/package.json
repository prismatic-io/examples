  "devDependencies": {
    "@prismatic-io/eslint-config-spectral": "2.0.2",
    "@types/jest": "29.5.11",
    "@types/ssh2-sftp-client": "^9.0.4",
    "copy-webpack-plugin": "11.0.0",
    "eslint": "8.57.0",
    "jest": "29.7.0",
    "node-loader": "^2.0.0",
    "ts-jest": "29.1.1",
    "ts-loader": "9.2.3",
    "typescript": "5.5.4",
    "webpack": "5.76.3",
    "webpack-cli": "5.0.1"
