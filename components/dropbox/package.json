{
  "name": "@prismatic-io/dropbox",
  "private": true,
  "version": "0.0.1",
  "description": "",
  "main": "index.ts",
  "author": "",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --cache --quiet --ext .ts,.js --ignore-pattern '*testapp*' .",
    "lint-fix": "eslint --cache --fix --quiet --ext .ts,.js --ignore-pattern '*testapp*' .",
    "format": "npm run lint-fix && prettier --loglevel error --write './src/**/*.{js,jsx,ts,tsx,css,html,json,scss}' 'documentation/*.mdx'",
    "check-format": "prettier --check './src/**/*.{js,jsx,ts,tsx,css,html,json,scss}' 'documentation/*.mdx'",
    "build": "npm run format && npm run lint && webpack",
    "test": "echo \"Warning: no test defined\"",
    "integration-test": "jest --maxWorkers=4",
    "testapp": "node ./test/testapp.js",
    "build:testapp": "npm run build && npm run testapp",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run build:testapp"
  },
  "dependencies": {
    "@prismatic-io/spectral": "7.3.3",
    "cross-fetch": "3.1.5",
    "dropbox": "10.34.0",
    "lodash.isequal": "4.5.0",
    "mime-types": "2.1.27"
  },
  "devDependencies": {
    "@prismatic-io/eslint-config-spectral": "1.0.0",
    "@types/express": "4.17.14",
    "@types/jest": "25.2.3",
    "@types/mime-types": "2.1.0",
    "@types/node": "14.14.35",
    "@types/node-fetch": "2.5.7",
    "body-parser": "1.20.1",
    "copy-webpack-plugin": "6.0.3",
    "dotenv": "8.2.0",
    "eslint": "6.8.0",
    "express": "4.18.2",
    "jest": "26.6.3",
    "nodemon": "2.0.20",
    "prettier": "2.0.5",
    "ts-jest": "26.4.0",
    "ts-loader": "8.0.2",
    "typescript": "4.3.4",
    "webpack": "5.88.2",
    "webpack-cli": "5.1.4"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@prismatic-io/eslint-config-spectral"
    ]
  }
}
